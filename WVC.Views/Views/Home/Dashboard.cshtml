@{ Layout = null; }
<div class="well-box-list">
	<div class="row">
		<div class="col-md-3 col-sm-4">
			<div class="well well-sm bg-cardovan">
				<div class="well-content">
					<h4>
						Airports</h4>
					<h2>
						<!-- ko if: total_airports()>0 -->
						<span data-bind="text:total_airports"></span>
						<!-- /ko -->
					</h2>
				</div>
				<div class="well-tolls">
					<a href="javascript:;" data-bind="click:listAirport" data-toggle="tooltip" data-placement="top" title="List"><i class="fa fa-list-alt"></i></a><a href="javascript:;" data-bind="click:function(data, event) { addAirport(); }" data-toggle="tooltip" data-placement="top" title="Edit"><i class="fa fa-pencil"></i></a><a href="javascript:;" data-bind="click:function(data, event) { addAirport(0) }" data-toggle="tooltip" data-placement="top" title="Add"><i class="fa fa-plus"></i></a>
				</div>
			</div>
		</div>
		<div class="col-md-3 col-sm-4">
			<div class="well well-sm bg-danger">
				<div class="well-content">
					<h4>
						Airlines</h4>
